# Content Indexing Worker - Environment Example
# Copy to .env and set values (used for local development only)

GOOGLE_CLOUD_PROJECT=your-gcp-project-id
LOCATION=us-central1
GCS_BUCKET_NAME=your-fiction-data-bucket

# Vertex AI Vector Search
VECTOR_SEARCH_INDEX_ID=your-index-id

# Embeddings
EMBEDDING_MODEL=text-embedding-004

# Chunking
CHUNK_SIZE=1200
CHUNK_OVERLAP=200
UPSERT_BATCH_SIZE=100
